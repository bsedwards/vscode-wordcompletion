{
    "name": "wordcomplete",
    "displayName": "wordcomplete",
    "description": "Eclipse-Like Word Completion",
    "version": "0.1.0",
    "publisher": "BrandonEdwards",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "keybindings": [
            {
                "command": "wordcompletion.cycle",
                "key": "alt+/",
                "when": "editorTextFocus && !editorHasMultiSelection && !editorReadOnly"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "lodash": "^4.17.11",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/lodash": "^4.14.118",
        "vsce": "^1.53.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bsedwards/vscode-wordcompletion"
    }
}
